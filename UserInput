import java.io.*;
public class WithUserInput {
public static void main(String [] args) throws IOException{
BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
	char let;
System.out.print("\n\tEnter letter: ");
let = br.readLine().charAt(0);
//System.out.print("\n" + let);
switch (let) {
	case 'a':
	case 'A':System.out.print("\napple");
		break;
	case 'b':
	case 'B':System.out.print("\nbanana");
		break;
	case 'c':
	case 'C':System.out.print("\ncherry");
		break;
	case 'd':
	case 'D':System.out.print("\nduhat");
		break;
	default: System.out.print ("\nNot in the list...");
		break;
}

}//main
}//class
