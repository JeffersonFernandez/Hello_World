//import java.io.*;
public class ForLoopSeven {
public static void main(String [] args) {
//BufferedReader br = new BufferedReader(new InputStreamReader(System.in));

for(int i= 0; i < 5; i++)
{
System.out.print("    *\n");
}
for(int j=0 ; j <=4; j++)
{
System.out.print("*");
}
System.out.print("\n");
}//main
}//class
