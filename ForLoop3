//no UI
import java.io.*;
public class ForLoopAlpha {
public static void main(String [] args){
//BufferedReader br = new BufferedReader(new //InputStreamReader(System.in));

for(int  i = 0; i < 10; i++){
System.out.print("\n");
System.out.print(" " + (char)('A' + i));
System.out.print("\n");
}//for loop
}//main
}//class
